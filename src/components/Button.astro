---
interface Props {
  title: string;
  bgcolor: string;
  textcolor: string;
  navigate: string;
  fontsize: string;
}

const { title, bgcolor, textcolor, navigate, fontsize } = Astro.props;
---

<a
  href={navigate}
  style={`background-color: ${bgcolor}`}
  class={`px-5 py-2 rounded-3xl text-${textcolor} ${fontsize} uppercase text-center z-20`}
>
  {title}</a
>
